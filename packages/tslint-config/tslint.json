{
  "extends": [
    "tslint-react",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-boolean-value": false,
    "jsx-no-bind": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "ordered-imports": true,
    "prefer-const": true,
    "prefer-object-spread": true
  }
}
